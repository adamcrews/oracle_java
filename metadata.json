{
  "name": "adamcrews-oracle_java",
  "version": "2.0.1",
  "author": "adamcrews",
  "summary": "Install and manage the Oracle version of the JDK and JRE.",
  "license": "Apache-2.0",
  "source": "https://github.com/adamcrews/oracle_java",
  "project_page": "https://github.com/adamcrews/oracle_java",
